{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "airbnb-base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "no-console": 0,
    "no-debugger": 2,
    "no-alert": 2,
    "no-unused-vars": 2,
    "no-undef": 2,
    "import/no-extraneous-dependencies": 0,
    "comma-dangle": 0,
    "global-require": 0,
    "import/no-unresolved": 0
  }
}
